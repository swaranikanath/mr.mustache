{
  "name": "mr-mustache-assistant",
  "displayName": "Mr Mustache Assistant",
  "publisher": "your-publisher-name",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "extension.js",
  "scripts": {
    "package": "vsce package || echo 'Install vsce to package extension'",
    "build": "npm run package",
    "prepare-devcontainer": "npm install && npm install -g vsce || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swaranika27nath-spec/mr.mustache"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:mr-mustache.suggest",
    "onCommand:mr-mustache.runCommand"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mr-mustache.suggest",
        "title": "Mr Mustache: Suggest Code"
      },
      {
        "command": "mr-mustache.runCommand",
        "title": "Mr Mustache: Run Command"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^2.0.0"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
